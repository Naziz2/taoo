{
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "target": "ES2020",
    "module": "ESNext",
    "lib": [
      "ES2020"
    ]
  },
  "extends": "expo/tsconfig.base",
  "exclude": [
    "node_modules",
    "src/web"
  ]
}
